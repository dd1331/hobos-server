{
  "name": "hobos-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --runInBand --watchAll",
    "typeorm": "typeorm-ts-node-commonjs -d ./data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate ./migrations/test",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.6",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/typeorm": "^8.1.2",
    "@nestjs/websockets": "^8.4.6",
    "@types/cache-manager": "^3.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/multer": "^1.4.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/passport-naver": "^0.2.1",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "aws-sdk": "^2.848.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^3.4.1",
    "cache-manager-redis-store": "^2.0.0",
    "cheerio": "^1.0.0-rc.5",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "dayjs": "^1.10.7",
    "express-session": "^1.17.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-naver": "^1.0.6",
    "pg": "^8.4.0",
    "random-words": "^1.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rss-parser": "^3.12.0",
    "rxjs": "^7.5.5",
    "typeorm": "0.3.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
